#!/usr/bin/expect
spawn bin/mini_RPG.exe
expect "What is your name? "
send "Andres Rodriguez\r"
expect "Is your name Andres Rodriguez? (yes/no) "
send "yes\r"
sleep .1
if { [lindex $argv 0] == "W" } {
    send "Warrior\r"
} elseif { [lindex $argv 0] == "S" } {
    send "Sorcerer\r"
} else {
    send "Hunter\r"
}
interact
